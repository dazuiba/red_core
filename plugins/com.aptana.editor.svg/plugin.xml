<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="svg"
            id="com.aptana.contenttype.svg"
            name="%content-type.svg.name"
            priority="normal">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.aptana.editor.svg.SVGSourceEditor"
            contributorClass="com.aptana.editor.common.CommonTextEditorActionContributor"
            default="true"
            extensions="svg"
            icon="icons/svg.png"
            id="com.aptana.editor.svg"
            name="%editor.svg.name">
      </editor>
   </extension>

</plugin>
